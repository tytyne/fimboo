{
  "name": "fimboo",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run migrations",
    "start": "babel-node src/index.js",
    "dev": "nodemon --exec babel-node  src/index.js",
    "create-db": "npx sequelize-cli db:create",
    "drop-db": "npx sequelize-cli db:drop",
    "migrations": "npx sequelize-cli db:migrate",
    "undo": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "undo-seed": "npx sequelize-cli db:seed:undo:all",
    "test": "cross-env NODE_ENV=test nyc mocha -r regenerator-runtime -r @babel/register --timeout 30000 --exit ./test/**/*.js",
    "pretest": "cross-env NODE_ENV=test npm run drop-db && cross-env NODE_ENV=test npm run create-db && cross-env NODE_ENV=test npm run migrations && cross-env NODE_ENV=test npm run seed",
    "coverage": "nyc --reporter=lcov npm test",
    "admin:create": "babel-node src/database/commands/superAdmin.js",
    "heroku-postbuild": "npm run role:create && npm run admin:create"
  },
  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/runtime": "^7.12.5",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.23.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-fileupload": "^1.2.1",
    "fs": "0.0.1-security",
    "http": "0.0.1-security",
    "i18n": "^0.13.2",
    "install": "^0.13.0",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.20",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.17",
    "nodemailer-express-handlebars": "^4.0.0",
    "nodemailer-sendgrid": "^1.0.3",
    "nodemon": "^2.0.7",
    "npm": "^6.14.11",
    "nyc": "^15.1.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "path": "^0.12.7",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "redis": "^3.1.2",
    "sendgrid": "^5.2.3",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "swagger-jsdoc": "^6.0.1",
    "swagger-ui-express": "^4.1.6",
    "twilio": "^3.60.0",
    "validator": "^13.5.2",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/register": "^7.12.10",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.2.1",
    "sinon": "^9.2.4"
  }
}
